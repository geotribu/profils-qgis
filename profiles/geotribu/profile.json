{
  "$schema": "https://raw.githubusercontent.com/Guts/qgis-deployment-cli/main/docs/schemas/profile/qgis_profile.json",
  "name": "Geotribu",
  "folder_name": "geotribu",
  "description": "Profil QGIS taill√© par et pour Geotribu.",
  "author": "Julien Moura",
  "email": "geotribu@gmail.com",
  "qgisMinimumVersion": "3.28.0",
  "qgisMaximumVersion": "3.99.10",
  "version": "1.2.0",
  "plugins": [
    {
      "name": "Layers menu from project",
      "folder_name": "menu_from_project",
      "official_repository": true,
      "plugin_id": 43,
      "version": "2.1.0"
    },
    {
      "name": "QTribu",
      "folder_name": "qtribu",
      "official_repository": true,
      "plugin_id": 2733,
      "version": "0.18.0"
    }
  ],
  "rules": [
    {
      "name": "2024+",
      "description": "Deploy only after 2024.",
      "conditions": {
        "all": [
          {
            "path": "$.date.current_year",
            "value": 2024,
            "operator": "greater_than"
          }
        ]
      }
    }
  ]
}
